@{
    ViewData["Title"] = "Dashboard";
    @model IEnumerable<Midas.Data.Entities.Ticker>;
}

<h1>@Model</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Ticker</th>
            <th scope="col">Company</th>
            <th scope="col">Exchange</th>
            <th scope="col">Price Currency</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var ticker in Model)
        {
            <tr>
                <td>@ticker.id</td>
                <td><a href="#">@ticker.ticker</a></td>
                <td>@ticker.Company.CompanyName</td>
                <td>@ticker.exchange</td>
                <td>@ticker.priceCurrency</td>
            </tr>
        }
    </tbody>
</table>